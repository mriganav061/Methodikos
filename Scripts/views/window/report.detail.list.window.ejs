<div id="report-detail-list-window" style='font-size:13px'>
	<div>Report for Detailed List of Funded Projects</div>
	<div id="report-detail-list-window-content">
		<fieldset><legend>List of Funded Projects:</legend>
	    <div id="tabbar-detail-project-list-panel">
			<div id="tabbar-detail-project-list">
				<ul>
					<li style='margin-left:15px'><%= data.Year+1 %></li>
					<li><%= data.Year+2 %></li>
					<li><%= data.Year+3 %></li>
					<li><%= data.Year+4 %></li>
				</ul>
				<!--<div id="tab-report-number-of-projects"><div id='number-projects-mnr-grid'></div></div>-->
				<div id="tab-report-detail-project-list-<%= data.Year+1 %>"><div id='detailed-list-projects-grid-<%= data.Year+1 %>'></div></div>
				<div id="tab-report-detail-project-list-<%= data.Year+2 %>"><div id='detailed-list-projects-grid-<%= data.Year+2 %>'></div></div>
				<div id="tab-report-detail-project-list-<%= data.Year+3 %>"><div id='detailed-list-projects-grid-<%= data.Year+3 %>'></div></div>
				<div id="tab-report-detail-project-list-<%= data.Year+4 %>"><div id='detailed-list-projects-grid-<%= data.Year+4 %>'></div></div>
			</div>
		</div>
	    </fieldset>
		<div style="float: right; margin-top: 10px;">
			<input type="button" id="report-detail-list-window-export" value="Export" />
			<input type="button" id="report-detail-list-window-ok" value="Close" />
		</div>
	</div>
</div>