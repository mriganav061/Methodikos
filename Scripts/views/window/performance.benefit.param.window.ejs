<div id="performance-benefit-opt-window" style='font-size:13px'>
	<div>Set Performance Benefit Options</div>
	<div id="performance-benefit-opt-window-content">
		<fieldset><legend>Options:</legend>
	    <div class='window-row'>
	        <label style='width:250px'>CS Threshold Value for Benefit Calculations:</label>
	        <div id='benefit-cs-threshold'></div>
	    </div>
	    <div class='window-row'>
	       <label style='width:250px'>M&R Trigger Parameter:</label>
	       <div id='mnr-trigger-param'></div>
	    </div>
	    
	    <div class='window-row'>
	       <label style='width:250px'>M&R Trigger Value:</label>
	       <div id='mnr-trigger-value'></div>
	    </div>

	    <div class='window-row'>
	       <label style='width:250px'>PM Viability Threshold:</label>
	       <div id='pm-viability-value'></div>
	    </div>

	    <div class='window-row'>
	       <label style='width:250px'>LR Viability Threshold:</label>
	       <div id='lr-viability-value'></div>
	    </div>
	    
	    </fieldset>
		<div style="float: right; margin-top: 10px;">
			<input type="button" id="performance-benefit-opt-window-restore" value="Set to Default" />
			<input type="button" id="performance-benefit-opt-window-ok" value="Save" />
			<input type="button" id="performance-benefit-opt-window-cancel" value="Cancel" />
		</div>
	</div>
</div>