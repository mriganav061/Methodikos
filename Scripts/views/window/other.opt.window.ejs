<div id="other-param-window" style='font-size:13px'>
	<div>Set Other Options</div>
	<div id="other-param-window-content">
		<fieldset style='margin-bottom:5px'><legend>Options:</legend>
	    <div class='window-row'>
	        <label style='width:150px'>Current Year:</label>
	        <div id='current-year'></div>
	    </div>
	    <div class='window-row'>
	        <label style='width:150px'>Year 1 Budget (million):</label>
	        <div id='year1-budget'></div>
	    </div>
	    <div class='window-row'>
	        <label style='width:150px'>Year 2 Budget (million):</label>
	        <div id='year2-budget'></div>
	    </div>
	    <div class='window-row'>
	        <label style='width:150px'>Year 3 Budget (million):</label>
	        <div id='year3-budget'></div>
	    </div>
	    <div class='window-row'>
	        <label style='width:150px'>Year 4 Budget (million):</label>
	        <div id='year4-budget'></div>
	    </div>
	    <div class='window-row'>
	        <label style='width:150px'>Discount Rate (%):</label>
	        <div id='discount-rate'></div>
	    </div>
	    <div class='window-row'>
	        <label style='width:150px'>AADT Compound Growth Rate (%):</label>
	        <div id='aadt-growth-rate'></div>
	    </div>
	    </fieldset>
	    
		<div style="float: right; margin-top: 10px;">
			<input type="button" id="other-param-window-restore" value="Set to Default" />
			<input type="button" id="other-param-window-ok" value="Save" />
			<input type="button" id="other-param-window-cancel" value="Cancel" />
		</div>
	</div>
</div>