<div id="report-network-condition-window" style='font-size:13px'>
	<div>Report for Network Condition</div>
	<div id="report-network-condition-window-content">
		<fieldset><legend>Options:</legend>
	   
	    
	    <div class='window-row'>
			<div id='chart-overall-network-condition-good-bad' style="width:680px; height:400px; position: relative; left: 0px; top: 0px;">
	    </div>
	    
	    <div class='window-row'>
	       <div id='chart-overall-network-condition-cs-classification' style="width:680px; height:400px; position: relative; left: 0px; top: 0px;">
	    </div>
	    <div class='window-row'>
	       	<div id='chart-backlog' style="width:680px; height:400px; position: relative; left: 0px; top: 0px;">
	    </div>
	     <div class='window-row'>
	        <div id='chart-network-prediction-ds' style="width:680px; height:400px; position: relative; left: 0px; top: 0px;">
		</div>
	    </div>
	    <div class='window-row'>
	       	<div id='chart-network-prediction-cs' style="width:680px; height:400px; position: relative; left: 0px; top: 0px;">
	    </div>
	    </fieldset>
		<div style="float: right; margin-top: 10px;">
			<input type="button" id="report-network-condition-window-ok" value="OK" />
			<input type="button" id="report-network-condition-window-cancel" value="Cancel" />
		</div>
	</div>
</div>